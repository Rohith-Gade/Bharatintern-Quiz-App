<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.root.quiz_app">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_background"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:configChanges="orientation|screenSize|keyboardHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>

        <activity android:name=".com.example.root.quiz_app_level.MainActivityStage2"/>
        <activity android:name=".com.example.root.quiz_app_level.ResultActivityStage2"/>
        <activity android:name=".com.example.root.quiz_app_level.ListActivityStage2"/>
        <activity android:name=".ResultActivity" />
        <activity android:name=".ListActivity" />


    </application>

</manifest>